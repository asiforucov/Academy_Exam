<!-- -----CONTACT----- -->
<section id="contact">
    <div class="row">
        <div class="container">
            <div class="header">
                <h2>CONTACT</h2>
                <h5>Sit sint consectetur velit quisquam cupiditate impedit suscipit alias</h5>
            </div>
            <div class="location">
                <div class="loc col-4">
                    <i class="fas fa-map-marker-alt fa-2x"></i>
                    <p>San Francisco, CA 94126, USA</p>
                </div>
                <div class="tel col-4">
                    <i class="fas fa-phone mt-4 fa-2x"></i>
                    <p>+ 01 234 567 89</p>
                </div>
                <div class="msg col-4">
                    <i class="fas fa-envelope mt-4 fa-2x"></i>
                    <p>contact@mdbootstrap.com</p>
                </div>

            </div>
            <form>
                <div class="form-row">
                    <div class="form-group col-md-6">
                        <input type="email" class="form-control" id="inputEmail4" placeholder="Your name">
                    </div>
                    <div class="form-group col-md-6">
                        <input type="password" class="form-control" id="inputPassword4" placeholder="Your email">
                    </div>
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" id="inputAddress" placeholder="Subject">
                </div>
                <div class="form-group">
                    <textarea class="form-control" id="exampleFormControlTextarea1" placeholder="Message" rows="3"></textarea>
                </div>


                <div class="button ">
                    <button type="submit" class="btn btn-primary ">Send message</button>

                </div>
            </form>
        </div>
    </div>
</section>
<!-- -----CONTACT----- -->
